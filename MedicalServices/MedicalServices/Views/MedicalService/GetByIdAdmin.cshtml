@model MedicalService
@{
    Layout = "Admin/_AdminLayout";
}

<div class="board">
    <table>
        <thead>
            <tr>
                <td>Name</td>
                <td>Description</td>
                <td>Branch</td>
                <td>Catigory</td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="title">
                    <div class="text">
                        <h5>@Model.Name</h5>
                    </div>
                </td>
                <td class="role">
                    <p class="text">@Model.Description</p>
                </td>
                <td class="role">
                    <p class="text">@(Model?.Branch?.Name ?? "Not Found")</p>
                </td>
                <td class="role">
                    <p class="text">@(Model?.Catigory?.Name ?? "Not Found")</p>
                </td>
            </tr>
        </tbody>
    </table>
</div>
